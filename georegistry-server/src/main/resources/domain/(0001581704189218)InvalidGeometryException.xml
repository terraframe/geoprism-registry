<version xsi:noNamespaceSchemaLocation="classpath:com/runwaysdk/resources/xsd/version.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <doIt>
    <create>
      <mdException name="net.geoprism.registry.io.InvalidGeometryException" 
                   exported="false"
                   publish="false"
                   label="Invalid Geometry Exception" 
                   description="Thrown when a geometry does not pass validation." 
                   message="The provided geometry is invalid. Check to ensure that the geometry is well formed and/or that it is in 4326 projection.">
      </mdException>
    </create>
    <update></update>
  </doIt>
  <undoIt>
    <delete>
      <object key="net.geoprism.registry.io.InvalidGeometryException" type="com.runwaysdk.system.metadata.MdException" />
    </delete>
  </undoIt>
</version>