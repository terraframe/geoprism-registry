            <operation name="READ" />
            <operation name="READ_ALL_ATTRIBUTES" />
          </mdRelationshipPermission>          
        </grant>
      </role>
    </permissions>        
  </doIt>
  <undoIt>
    <delete>
    </delete>
  </undoIt>
